# Set architecture flags
export ARCHFLAGS="-arch x86_64"

export HEROKU_HOME="/usr/local/heroku/bin"
export PROJECT_HOME=$HOME/code/projects

export GOPATH=$HOME/go

export LANG="en_US.UTF-8"
export LC_COLLATE="en_US.UTF-8"
export LC_CTYPE="en_US.UTF-8"
export LC_MESSAGES="en_US.UTF-8"
export LC_MONETARY="en_US.UTF-8"
export LC_NUMERIC="en_US.UTF-8"
export LC_TIME="en_US.UTF-8"
export LC_ALL=

POSTGRES="/Applications/Postgres.app/Contents/Versions/latest/bin"

USR_PATH="/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"

ADD_PATH="$POSTGRES:$GOPATH"

export PATH=$USR_PATH:$ADD_PATH
